    "@vue/cli-plugin-typescript": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue-async/module-loader":"^3.1.4",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.11.1",
