{
  "name": "admin-ui",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aceHubert/vue-async.git"
  },
  "scripts": {
    "postinstall": "concurrently --raw \"cd ./modules && yarn install\" \"cd ./admin-ui && yarn install\"",
    "serve": "concurrently --raw \"yarn serve:modules\" \"yarn serve:admin-ui\"",
    "serve:admin-ui": "ccd ./admin-ui && yarn serve",
    "serve:modules": "cd ./modules && yarn serve",
    "build": "concurrently --raw \"yarn build:modules\" \"yarn build:admin-ui\"",
    "build:admin-ui": "cd ./admin-ui && yarn build",
    "build:modules": "cd ./modules && yarn build",
    "prettier": "node ./scripts/prettier.js",
    "prettier:all": "node ./scripts/prettier.js write",
    "changelog": "node ./scripts/genChangelog.js",
    "commit": "git-cz"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "child_process": "^1.0.2",
    "commitizen": "^4.0.3",
    "concurrently": "^5.1.0",
    "conventional-changelog": "^3.1.21",
    "core-js": "^3.6.5",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^2.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "glob-gitignore": "^1.0.14",
    "husky": ">=1",
    "jest": "^24.9.0",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2",
    "util": "^0.12.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
