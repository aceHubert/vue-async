{
  "name": "@vue-async/resource-manager",
  "version": "1.0.16",
  "author": "Hubert<yi.xiang@live.com>",
  "description": "Resource Manager.",
  "main": "lib/index.js",
  "umd:main": "dist/resource-manager.umd.production.js",
  "module": "esm/index.js",
  "unpkg": "dist/resource-manager.umd.production.js",
  "jsdelivr": "dist/resource-manager.umd.production.js",
  "keywords": [
    "vue",
    "vue-async",
    "resource-mananger",
    "suspense"
  ],
  "files": [
    "dist",
    "lib",
    "esm",
    "types"
  ],
  "scripts": {
    "serve": "nuxt dev playground -p 7003",
    "build": "run -T rimraf -rf lib esm dist types && yarn build:version && yarn build:cjs && yarn build:esm && yarn build:umd",
    "build:version": "node -p \"'export const version: string = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build:cjs": "run -T tsc --project tsconfig.build.json",
    "build:esm": "run -T tsc --project tsconfig.build.json --module es2015 --outDir esm",
    "build:umd": "run -T rollup --config",
    "lint": "run -T eslint . --cache --report-unused-disable-directives --ignore-path=../../.eslintignore",
    "lint:fix": "run -T eslint . --cache --fix --ignore-path=../../.eslintignore"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aceHubert/vue-async.git",
    "directory": "packages/resource-mananger"
  },
  "bugs": {
    "url": "https://github.com/aceHubert/vue-async/issues"
  },
  "dependencies": {
    "@ace-util/core": "^0.2.0",
    "vue-demi": "*",
    "vue-frag": "^1.4.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@types/warning": "^3.0.0",
    "nuxt": "^3.0.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0",
    "vue": "^2.6.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
