{
  "name": "@vue-async/module-loader",
  "version": "1.0.5",
  "author": "Hubert<yi.xiang@live.com>",
  "description": "Module Loader Core.",
  "main": "module-loader.common.ts",
  "umd:main": "dist/module-loader.umd.js",
  "module": "dist/module-loader.esm.js",
  "unpkg": "dist/module-loader.js",
  "jsdelivr": "dist/module-loader.js",
  "typings": "types/module-loader.d.ts",
  "keywords": [
    "vue",
    "vue-async",
    "module-loader",
    "async-module",
    "async-loader",
    "microfront"
  ],
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "scripts": {
    "serve": "rollup -c ./rollup.config.dev.js -w",
    "build": "rollup -c",
    "build:es": "cross-env TARGET=es rollup -c",
    "release": "bash scripts/release.sh",
    "test": "cd ../../ && cross-env NODE_ENV=test jest --passWithNoTests --config packages/module-loader/jest.config.json",
    "lint": "eslint . --cache --report-unused-disable-directives --ignore-path=../../.eslintignore",
    "lint:fix": "eslint . --cache --fix --ignore-path=../../.eslintignore"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aceHubert/vue-async.git",
    "directory": "packages/module-loader"
  },
  "bugs": {
    "url": "https://github.com/aceHubert/vus-async/issues"
  },
  "dependencies": {
    "@vue-async/utils": "^1.0.4"
  },
  "devDependencies": {
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "peerDependencies": {
    "vue": "^2.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
