{
  "name": "@vue-async/fetch-axios",
  "version": "1.0.0-alpha.0",
  "author": "Hubert<yi.xiang@live.com>",
  "description": "Axios adapter for Ajax.",
  "main": "lib/index.js",
  "umd:main": "dist/fetch-axios.umd.production.js",
  "module": "esm/index.js",
  "unpkg": "dist/fetch-axios.umd.production.js",
  "jsdelivr": "dist/fetch-axios.umd.production.js",
  "typings": "types/index.d.ts",
  "keywords": [
    "vue",
    "vue-async",
    "fetch",
    "axios",
    "microfront"
  ],
  "files": [
    "dist",
    "lib",
    "esm"
  ],
  "scripts": {
    "postinstall": "fetch-vue-fix",
    "serve": "concurrently --raw \"cd ../../demo/mock-data && yarn serve\" \"tsc --project tsconfig.build.json --module es2015 --outDir esm -watch\" \"tsc --project tsconfig.types.json -watch\" \"node ./dev/server.js\"",
    "prebuild": "node -p \"'export const version: string = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "rimraf -rf lib esm dist types && yarn build:cjs && yarn build:esm && yarn build:umd && yarn build:types",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --outDir esm",
    "build:umd": "rollup --config",
    "build:types": "tsc --project tsconfig.types.json",
    "build:copy-dts": "copyfiles -u 1 src/**/*.d.ts types",
    "lint": "eslint . --cache --report-unused-disable-directives --ignore-path=../../.eslintignore",
    "lint:fix": "eslint . --cache --fix --ignore-path=../../.eslintignore"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aceHubert/vue-async.git",
    "directory": "packages/fetch-axios"
  },
  "bugs": {
    "url": "https://github.com/aceHubert/vus-async/issues"
  },
  "dependencies": {
    "@vue-async/fetch": "1.0.0-alpha.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/composition-api": "^0.32.0",
    "@types/warning": "^3.0.0",
    "@vue/composition-api": "^1.7.0",
    "axios": "^0.21.1",
    "nuxt": "^2.14.6",
    "vue": "^2.6.12"
  },
  "peerDependencies": {
    "axios": "^0.21.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
