{
  "name": "@vue-async/components",
  "version": "1.0.0-alpha.1",
  "author": "Hubert<yi.xiang@live.com>",
  "description": "A config editor component.",
  "main": "lib/index.js",
  "umd:main": "dist/components.umd.js",
  "module": "esm/index.js",
  "unpkg": "dist/components.umd.production.js",
  "jsdelivr": "dist/components.umd.production.js",
  "keywords": [
    "vue",
    "vue-async",
    "complex components",
    "josn schema",
    "data scoped"
  ],
  "files": [
    "dist/*.js",
    "types/*.d.ts"
  ],
  "scripts": {
    "dev": "concurrently --raw \"tsc --project tsconfig.build.json --module es2015 --outDir esm --watch\" \"ts-node create-style && ts-node build-style watch\" \"node ./dev/server.js\"",
    "prebuild": "node -p \"'export const version: string = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "rimraf -rf lib esm dist types && npm run build:cjs && npm run build:esm && npm run build:umd",
    "create:style": "ts-node create-style",
    "build:style": "ts-node build-style",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:esm": "ttsc --project tsconfig.build.json --module es2015 --outDir esm",
    "build:umd": "rollup --config",
    "build:js": "cross-env BABEL_ENV=esm babel src --root-mode upward --out-dir esm --extensions \".ts,.tsx\" --source-maps inline --verbose",
    "lint": "eslint . --cache --report-unused-disable-directives --ignore-path=../../.eslintignore",
    "lint:fix": "eslint . --cache --fix --ignore-path=../../.eslintignore"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aceHubert/vue-async.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/aceHubert/vus-async/issues"
  },
  "dependencies": {
    "@formily/core": "^2.0.18",
    "@formily/vue": "^2.0.18",
    "@monaco-editor/loader": "^1.3.1",
    "lodash.merge": "^4.6.2",
    "lodash.startcase": "^4.4.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@formily/antdv": "^1.0.0-alpha.7",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.startcase": "^4.4.6",
    "@types/warning": "^3.0.0",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/composition-api": "^1.4.9",
    "ant-design-vue": "^3.1.1",
    "autoprefixer": "^9.8.6",
    "monaco-editor": "0.30.1",
    "monaco-editor-webpack-plugin": "6.0.0",
    "nuxt": "^2.14.6",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.11",
    "vue3": "npm:vue@3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "monaco-editor": ">= 0.25.0 < 1",
    "vue": "^2.6.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "monaco-editor": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
